@using AirWeb.WebApp.Platform.PageModelHelpers
@using GaEpd.AppLibrary.Extensions
@model IList<IaipDataService.Facilities.AirProgramClassification>
@if (Model.Count == 0)
{
    <span class="text-body-secondary fw-lighter">@DefaultText.PlaceholderNone</span>
}
else
{
    <ul class="mb-0 ms-n2">
        @foreach (var classification in Model)
        {
            <li class="float-start me-4 ms-2">@classification.GetDescription()</li>
        }
    </ul>
}
