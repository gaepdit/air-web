@using AirWeb.WebApp.Platform.PageModelHelpers
@model IList<IaipDataService.Facilities.Pollutant>
@if (Model.Count == 0)
{
    <span class="text-body-secondary fw-lighter">@DefaultText.PlaceholderNone</span>
}
else
{
    <ul>
        @foreach (var pollutant in Model)
        {
            <li>@pollutant.Description <span class="text-body-tertiary">(@pollutant.Code)</span></li>
        }
    </ul>
}
