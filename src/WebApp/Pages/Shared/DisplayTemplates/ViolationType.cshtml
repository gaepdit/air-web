@using AirWeb.WebApp.Platform.PageModelHelpers
@model AirWeb.Domain.EnforcementEntities.ViolationTypes.ViolationType?
@if (Model is null)
{
    <span class="text-body-secondary fw-lighter">@DefaultText.PlaceholderNotEntered</span>
}
else
{
    @Model.SeverityCode<text>: </text>@Model.Description
    <span class="text-body-tertiary">(@Model.Code)</span>
    
}
