@model AirWeb.WebApp.Platform.PageModelHelpers.ISubmitCancelButtons

<div class="mt-2">
    <button id="SubmitButton" type="submit" class="btn btn-primary me-2">@Model.SubmitText</button>
    <a asp-page="@Model.CancelRoute" asp-route-id="@Model.RouteId" class="btn btn-outline-secondary">Cancel</a>
</div>
<div class="text-danger-emphasis mt-3">* denotes a required field</div>
