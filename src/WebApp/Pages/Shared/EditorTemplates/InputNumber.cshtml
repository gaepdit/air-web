@model int
@{
    var min = (int?)ViewData["Min"];
    var max = (int?)ViewData["Max"];
}
<div class="form-floating">
    <input asp-for="@__model" class="form-control" min="@min" max="@max" />
    <label asp-for="@__model" class="form-label"></label>
    <span asp-validation-for="@__model" class="invalid-feedback"></span>
</div>
