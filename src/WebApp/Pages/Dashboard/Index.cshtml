@page
@model DashboardIndexModel
@{
    ViewData["Title"] = "Staff Dashboard";
}

<h1>@ViewData["Title"]</h1>

@if (Model.IsStaff)
{
    @foreach (var card in Model.DashboardCards)
    {
        <partial name="Compliance/_Partials/_DashboardCardWorkEntry" model="card" />
    }
}
else
{
    <p>See your manager to discuss application access if needed.</p>
}
